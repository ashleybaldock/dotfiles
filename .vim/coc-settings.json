{
  "diagnostic.enableSign": true,
  "list.signOffset": 1,
  "diagnostic.errorSign": "‚úò",
  "diagnostic.warningSign": "‚ÄºÔ∏é",
  "diagnostic.infoSign": "ùì≤",
  "diagnostic.hintSign": "‚åÅ",

  "suggest.noselect": true,
  "diagnostic.signLevel": "warning",
  "diagnostic.signPriority": 10,

  "suggest.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 40
  },

  "diagnostic.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 80
  },

  "signature.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 40
  },

  "hover.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 90
  },
  

  "prettier.requireConfig": true,

  "eslint.autoFixOnSave": true,

  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.extensionUpdateCheck": "never",

  "diagnostic.checkCurrentLine": true,

  "hover.target": "float",
  "coc.preferences.formatOnSave": true,
  "json.format.enable": true,
  "javascript.format.enable": false,
  "prettier.formatterPriority": 1,
  "prettier.enable": true,

  "typescript.preferences.prefer TypeOnlyAutoImports": true
}
