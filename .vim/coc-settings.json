{
  "coc.preferences.renameFillCurrent": false,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.formatOnSave": true,

  "list.signOffset": 1,
  "list.floatPreview": false,
  "css.lint.float": "warning",
  "css.lint.zeroUnits": "warning",
  "css.format.braceStyle": "collapse",
  "css.lint.duplicateProperties": "warning",
  "css.format.newlineBetweenSelectors": true,
  "css.lint.propertyIgnoredDueToDisplay": "warning",

  "suggest.noselect": true,
  "suggest.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 40
  },

  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableSign": true,
  "diagnostic.signLevel": "warning",
  "diagnostic.signPriority": 10,
  "diagnostic.errorSign": "‚úò",
  "diagnostic.warningSign": "‚ÄºÔ∏é",
  "diagnostic.infoSign": "ùì≤",
  "diagnostic.hintSign": "‚åÅ",
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 80
  },

  "signature.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 40
  },

  "hover.target": "float",
  "hover.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "HlPop01Bg",
    "borderhighlight": "HlPop01Bd",
    "maxWidth": 90
  },
  
  "eslint.autoFixOnSave": true,

  "json.format.enable": true,

  "javascript.format.enable": false,

  "prettier.requireConfig": true,
  "prettier.formatterPriority": 1,
  "prettier.enable": true,

  "typescript.preferences.prefer TypeOnlyAutoImports": true
}
