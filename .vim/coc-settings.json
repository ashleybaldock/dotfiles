{
  "coc.preferences.renameFillCurrent": true,
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.bracketEnterImprove": false,
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.preferences.currentFunctionSymbolDebounceTime": 300,
  "coc.preferences.maxFileSize": "10MB",

  "workspace.rootPatterns": [".git", ".root"],
  "workspace.ignoredFolders": [],
  "workspace.ignoredFiletypes": [".DS_Store", ".swo", ".swp", ".swn"],

  "list.signOffset": 1,
  "list.floatPreview": true,
  "list.height": 10,
  "list.maxPreviewHeight": 10,
  "list.indicator": ">",
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "list.alignColumns": false,
  "list.selectedSignText": "*",
  "list.previewSplitRight": false,
  "list.extendedSearchMode": true,
  "list.previewHighlightGroup": "Search",
  "list.interactiveDebounceTime": 100,
  "list.source.diagnostics.pathFormat": "short",
  "list.statusLineSegments": [
    "%#CocListMode#-- %{coc#list#status(\"mode\")\\} --%*",
    "%{coc#list#status(\"loading\")\\}",
    "%{coc#list#status(\"args\")\\}",
    "(%L/%{coc#list#status(\"total\")\\})",
    "%=",
    "%#CocListPath# %{coc#list#status(\"cwd\")\\} %l/%L%*"
  ],

  "suggest.timeout": 5000,
  "suggest.selection":"first",
  "suggest.asciiMatch": true,
  "suggest.insertMode": "replace",
  "suggest.autoTrigger": "always",
  "suggest.detailField": "preview",
  "suggest.enableFloat": true,
  "suggest.virtualText": false,
  "suggest.ignoreRegexps": [],
  "suggest.localityBonus": true,
  "suggest.filterGraceful": true,
  "suggest.labelMaxLength": 200,
  "suggest.enablePreselect": true,
  "suggest.snippetsSupport": true,
  "suggest.snippetIndicator": "~",
  "suggest.defaultSortMethod": "length",
  "suggest.filterOnBackspace": true,
  "suggest.removeCurrentWord": false,
  "suggest.asciiCharactersOnly": false,
  "suggest.maxCompleteItemCount": 256,
  "suggest.removeDuplicateItems": false,
  "suggest.minTriggerInputLength": 1,
  "suggest.reversePumAboveCursor": false,
  "suggest.triggerCompletionWait": 0,
  "suggest.languageSourcePriority": 99,
  "suggest.lowPrioritySourceLimit": 2,
  "suggest.highPrioritySourceLimit": 5,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.completionItemKindLabels": {},
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.noselect": true,
  "suggest.floatConfig": {
    "rounded": false,
    "border": false,
    "highlight": "HlCocPuSugsBg",
    "borderhighlight": "HlCocPuSugsBd",
    "maxWidth": 40
  },

  "diagnostic.checkCurrentLine": false,
  "diagnostic.enableSign": true,
  "diagnostic.signLevel": "hint",
  "diagnostic.signPriority": 4,
  "diagnostic.errorSign": "‚úò",
  "diagnostic.warningSign": "‚ÄºÔ∏é",
  "diagnostic.infoSign": "ùì≤",
  "diagnostic.hintSign": "‚åÅ",
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": false,
    "title": "Coc Diagnostic",
    "highlight": "HlCocPuDiagBg",
    "borderhighlight": "HlCocPuDiagBd",
    "maxWidth": 80
  },

  "signature.enable": true,
  "signature.target": "float",
  "signature.hideOnTextChange": true,
  "signature.preferShownAbove": true,
  "signature.triggerSignatureWait": 500,
  "signature.floatConfig": {
    "border": true,
    "rounded": true,
    "title": "Coc Signature",
    "highlight": "HlCocPuSgtrBg",
    "borderhighlight": "HlCocPuSgtrBd",
    "maxWidth": 40
  },

  "hover.autoHide": true,
  "hover.previewMaxHeight": 12,
  "hover.target": "float",
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "title": "Coc Hover",
    "highlight": "HlCocPuHovrBg",
    "borderhighlight": "HlCocPuHovrBd",
    "maxWidth": 90
  },
  
  "css.lint.float": "warning",
  "css.lint.zeroUnits": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.propertyIgnoredDueToDisplay": "warning",
  "css.format.braceStyle": "collapse",
  "css.format.newlineBetweenSelectors": true,

  "yank.byteLengthLimit": 1024,
  "yank.enableCompletion": true,
  "yank.highlight.duration": 100,
  "yank.highlight.enable": false,
  "yank.limit": 3,
  "yank.list.maxsize": 300,
  "yank.priority": 90,

  "eslint.autoFixOnSave": true,

  "json.format.enable": true,

  "javascript.format.enable": false,
  "javascript.preferGoToSourceDefinition": false,

  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade"
  ],
  "html.autoClosingTags": true,
  "html.validate.styles": true,
  "html.validate.scripts": true,
  "html.hover.references": true,
  "html.format.extraLiners": "head, body, /html",
  "html.format.contentUnformatted": "pre,code,textarea",
  "html.completion.attributeDefaultValue": "doublequotes",

  "prettier.requireConfig": true,
  "prettier.formatterPriority": 1,
  "prettier.enable": true,

  "typescript.preferences.prefer TypeOnlyAutoImports": true
}
