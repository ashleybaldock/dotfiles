{
  "coc.preferences.renameFillCurrent": true,
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.bracketEnterImprove": false,
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.preferences.currentFunctionSymbolDebounceTime": 300,
  "coc.preferences.maxFileSize": "10MB",
  "semanticTokens.enable": false,

  "workspace.rootPatterns": [".git", ".root"],
  "workspace.ignoredFolders": [],
  "workspace.ignoredFiletypes": [".DS_Store", ".swo", ".swp", ".swn"],

  "list.signOffset": 1,
  "list.floatPreview": true,
  "list.height": 10,
  "list.maxPreviewHeight": 10,
  "list.indicator": ">",
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "list.alignColumns": false,
  "list.selectedSignText": "*",
  "list.previewSplitRight": false,
  "list.extendedSearchMode": true,
  "list.previewHighlightGroup": "Search",
  "list.interactiveDebounceTime": 100,
  "list.source.diagnostics.pathFormat": "short",
  "list.statusLineSegments": [
    "%{%coc#list#status('loading')%}",
    "(%L/%{%coc#list#status('total')%})",
    "%{%coc#list#status('args')%}",
    "%=",
    "%#CocListPath# %{%coc#list#status('cwd')%} %l/%L%*"
  ],

  "suggest.timeout": 5000,
  "suggest.selection": "first",
  "suggest.asciiMatch": true,
  "suggest.insertMode": "replace",
  "suggest.autoTrigger": "always",
  "suggest.detailField": "preview",
  "suggest.enableFloat": true,
  "suggest.virtualText": false,
  "suggest.ignoreRegexps": [],
  "suggest.localityBonus": true,
  "suggest.filterGraceful": true,
  "suggest.labelMaxLength": 200,
  "suggest.enablePreselect": true,
  "suggest.snippetsSupport": true,
  "suggest.snippetIndicator": "~",
  "suggest.defaultSortMethod": "length",
  "suggest.filterOnBackspace": true,
  "suggest.removeCurrentWord": false,
  "suggest.asciiCharactersOnly": false,
  "suggest.maxCompleteItemCount": 256,
  "suggest.removeDuplicateItems": false,
  "suggest.minTriggerInputLength": 1,
  "suggest.reversePumAboveCursor": false,
  "suggest.triggerCompletionWait": 0,
  "suggest.languageSourcePriority": 99,
  "suggest.lowPrioritySourceLimit": 2,
  "suggest.highPrioritySourceLimit": 5,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.completionItemKindLabels": {
    "default": "ô€ ",
    "file": "ô€ˆ·",
    "folder": "ô€ˆ•",
