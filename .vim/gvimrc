"
" Edit Menu:
"
noremenu  20.402 &Edit.&ReSelect\ Paste<Tab>gp	gp
noremenu  20.403 &Edit.&ReSelect\ Visual<Tab>gv	gv

"
" HIG Cmd And Option Movement Mappings:
" This replaces the section in $VIMRUNTIME/gvimrc, which
" is disabled via this flag in ./vimrc
"   let macvim_hig_shift_movement = 1
"
if has("gui_macvim")
  "
  " ▌️􀆔 􁾱  ▐️ ️Move to the ends of things

  " ▌️􀆔 􁾲  ▐️
  noremap <D-Up> <C-Home>

  " ▌️􀆔 􁾵  ▐️
  noremap   <D-Right>  <End>
  noremap!  <D-Right>  <End>

  " ▌️􀆔 􁾳  ▐️ 
  noremap   <D-Down>   <C-End>

  " ▌️􀆔 􁾴  ▐️
  noremap   <D-Left>   <Home>
  noremap!  <D-Left>   <Home>

  " ▌️􀆕 􁾱  ▐️ Move in steps

  " ▌️􀆕 􁾲  ▐️
  " noremap <M-Up> {

  " ▌️􀆕 􁾵  ▐️
  noremap  <M-Right> <C-Right>
  noremap! <M-Right> <C-Right>

  " ▌️􀆕 􁾳  ▐️
  " noremap <M-Down> }

  " ▌️􀆕 􁾴  ▐️
  noremap  <M-Left> <C-Left>
  noremap! <M-Left> <C-Left>


  "
  " Insert Mode:
  "

  " ▌️􀆔 􁾲  ▐️ PUM: Jump to top
  inoremap <expr> <D-Up>     pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-Home>"
  " ▌️􀆔 􁾵  ▐️ PUM: Accept selection + exit
  inoremap <expr> <D-Right>  pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-End>"
  " ▌️􀆔 􁾳  ▐️ PUM: Jump to end
  inoremap <expr> <D-Down>   pumvisible() ? "\<PageDown>\<C-p>\<C-n>" : "\<C-End>"
  " ▌️􀆔 􁾴  ▐️ PUM: Cancel out
  inoremap <expr> <D-Left>   pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-Home>"

  " ▌️􀆔 􀆛 ▐️ ️
  inoremap <D-BS> <C-u>

  " ▌️􀆕 􀆛 ▐️
  inoremap <M-BS> <C-w>

  " \_^\ze\s*\S\|\S\ze\s\|\s\zs\S\|\S\s*\zs\_$

  " ▌️􀆕 􁾲  ▐️ PUM: Up 1 page
  inoremap <expr> <M-Up> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-o>:move -2<CR>"
  " ▌️􀆕 􁾵  ▐️ PUM: Select path part & prompt for next part
  inoremap <expr> <M-Right> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-o>w"
  " ▌️􀆕 􁾳  ▐️ PUM: Down 1 page
  inoremap <expr> <M-Down> pumvisible() ? "\<PageDown>\<C-p>\<C-n>" : "\<C-o>:move +1<CR>"
  " ▌️􀆕 􁾴  ▐️ PUM: Jump up a level in path, and reselect
  inoremap <expr> <M-Left> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-o>b"
endif

" ^X  open
" ^P  prev (preview inline)
" ^N  next (preview inline)
" ^Y  accept
" ^E  cancel



" if has("gui_macvim")
  " Shift + special movement key (<S-Left>, etc.)
  " and mouse starts insert mode
  " set selectmode=mouse,key
  " set keymodel=startsel,stopsel

  " nn   <S-D-Left>     <S-Home>
  " vn   <S-D-Left>     <S-Home>
  " ino  <S-D-Left>     <S-Home>
  " nn   <S-M-Left>     <S-C-Left>
  " vn   <S-M-Left>     <S-C-Left>
  " ino  <S-M-Left>     <S-C-Left>

  " nn   <S-D-Right>    <S-End>
  " vn   <S-D-Right>    <S-End>
  " ino  <S-D-Right>    <S-End>
  " nn   <S-M-Right>    <S-C-Right>
  " vn   <S-M-Right>    <S-C-Right>
  " ino  <S-M-Right>    <S-C-Right>

  " nn   <S-D-Up>       <S-C-Home>
  " vn   <S-D-Up>       <S-C-Home>
  " ino  <S-D-Up>       <S-C-Home>

  " nn   <S-D-Down>     <S-C-End>
  " vn   <S-D-Down>     <S-C-End>
  " ino  <S-D-Down>     <S-C-End>
" endif


