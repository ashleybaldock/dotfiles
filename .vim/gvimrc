" HIG Cmd And Option Movement Mappings:
" This replaces the section in $VIMRUNTIME/gvimrc, which
" is disabled via this flag in ./vimrc
"   let macvim_hig_shift_movement = 1
"
if has("gui_macvim")
  "
  " â–Œï¸âŒ˜ï¸ ô¾±  â–ï¸ ï¸Move to the ends of things

  " â–Œï¸ô€†” ô¾²  â–ï¸â”€â”€â”€â”€â–· Jump to top of PUM
  noremap <D-Up> <C-Home>
  " â–Œï¸ô€†” ô¾²  â–ï¸â”€â”€â”€â”€â–· Jump to top of PUM
  inoremap <expr> <D-Up>     pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-Home>"

  "ô€†”ô¾µ 
  noremap   <D-Right>  <End>
  noremap!  <D-Right>  <End>
" Accept selection + exit PUM
  inoremap <expr> <D-Right>  pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-Home>"

  noremap   <D-Down>   <C-End>
  inoremap  <D-Down>   <C-End>
  " â–Œï¸ô€†” ô¾³  â–ï¸ Jump to end of PUM
  inoremap <expr> <D-Down>   pumvisible() ? "\<PageDown>\<C-p>\<C-n>" : "\<C-End>"

  noremap   <D-Left>   <Home>
  noremap!  <D-Left>   <Home>
  " â–Œï¸ô€†” ô¾´  â–ï¸ Cancel out of PUM
  inoremap <expr> <D-Left>   pumvisible() ? "\<PageUp>\<C-p>\<C-n>"   : "\<C-Home>"

  " â–Œï¸ô€†” ô€†› â–ï¸ ï¸
  inoremap  <D-BS> <C-u>

  "
  " â–Œï¸ô€†• ô¾±  â–ï¸ Move (around) things in steps

  " â–Œï¸ô€†• ô¾±  â–ï¸ Move in steps
  " noremap <M-Up> {
  inoremap <M-Up> <C-o>{

  noremap  <M-Right> <C-Right>
  noremap! <M-Right> <C-Right>

  " noremap <M-Down> }
  inoremap <M-Down> <C-o>}

  noremap  <M-Left> <C-Left>
  noremap! <M-Left> <C-Left>

  inoremap <M-BS> <C-w>

" â–Œï¸ô€†• ô¾²  â–ï¸â”€â”€â”€â”€â–· Jump up a page in PUM listing
  inoremap <expr> <M-Up> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-o>{"
" â–Œï¸ô€†• ô¾µ  â–ï¸â”€â”€â”€â”€â–· Select part of path in PUM + prompt for next part
  inoremap <expr> <M-Right> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-Home>"
" â–Œï¸ô€†• ô¾³  â–ï¸â”€â”€â”€â”€â–· Jump down a page in PUM listing
  inoremap <expr> <M-Down> pumvisible() ? "\<PageDown>\<C-p>\<C-n>" : "\<C-End>"
" â–Œï¸ô€†• ô¾´  â–ï¸â”€â”€â”€â”€â–· Jump up a level in path, and reselect
  inoremap <expr> <M-Left> pumvisible() ? "\<PageUp>\<C-p>\<C-n>" : "\<C-o>}"
endif

" ^X  open
" ^P  prev (preview inline)
" ^N  next (preview inline)
" ^Y  accept
" ^E  cancel



" if has("gui_macvim")
  " Shift + special movement key (<S-Left>, etc.) and mouse starts insert mode
  " set selectmode=mouse,key
  " set keymodel=startsel,stopsel

  " nn   <S-D-Left>     <S-Home>
  " vn   <S-D-Left>     <S-Home>
  " ino  <S-D-Left>     <S-Home>
  " nn   <S-M-Left>     <S-C-Left>
  " vn   <S-M-Left>     <S-C-Left>
  " ino  <S-M-Left>     <S-C-Left>

  " nn   <S-D-Right>    <S-End>
  " vn   <S-D-Right>    <S-End>
  " ino  <S-D-Right>    <S-End>
  " nn   <S-M-Right>    <S-C-Right>
  " vn   <S-M-Right>    <S-C-Right>
  " ino  <S-M-Right>    <S-C-Right>

  " nn   <S-D-Up>       <S-C-Home>
  " vn   <S-D-Up>       <S-C-Home>
  " ino  <S-D-Up>       <S-C-Home>

  " nn   <S-D-Down>     <S-C-End>
  " vn   <S-D-Down>     <S-C-End>
  " ino  <S-D-Down>     <S-C-End>
" endif

